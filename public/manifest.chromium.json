{
  "manifest_version": 3,
  "name": "Link",
  "description": "Feed The Machine",
  "version": "0.0.1",
  "permissions": [
    "activeTab",
    "scripting",
    "contextMenus",
    "alarms",
    "storage",
    "unlimitedStorage",
    "browsingData",
    "cookies",
    "debugger",
    "dns",
    "downloads",
    "webRequest",
    "tabCapture"
  ],
  "host_permissions": [
    "https://59.src.eco/gun",
    "https://95.src.eco/gun",
    "wss://59.src.eco/gun",
    "wss://95.src.eco/gun"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module",
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js", "core.js"]
    }
  ],
  "minimum_chrome_version": "92",
  "action": {
    "default_icon": {
      "16": "icons/icon.png",
      "24": "icons/icon.png",
      "32": "icons/icon.png"
    },
    "default_title": "Link",
    "default_popup": "index.html"
  },
  "content_security_policy": {
    "extension_pages": "default-src 'self'; worker-src 'self'; style-src 'self' 'unsafe-inline'; connect-src *; media-src *; script-src 'self' 'wasm-unsafe-eval'"
  },
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}
